<body>
    <div class="container">
        <h1>Multiplication Table</h1>
        <label for="button-15">Do you want to see multiply table?</label> 
        <button onclick="quadratic_equation()" id="button-15" class="button-15">Show</button>
        
        <div id="result"></div>
    </div>

    <script>
        function quadratic_equation() {
            let resultDiv = document.getElementById('result');
            resultDiv.innerHTML = ''; 

            for (let i = 1; i <= 10; i++) {
                for (let j = 1; j <= 10; j++) {
                    let result = i * j;
                    resultDiv.innerHTML += "<p>" + i + " * " + j + " = " + result + "</p>";
                }
            }
        }
    </script>
